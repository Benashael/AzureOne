{
  "bindings": [
    {
      "name": "myBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "uploads/{blobName}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "direction": "out",
      "path": "uploads/{blobName}",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "index.js"
}
